{
  "name": "helm-highlight-unused-values",
  "displayName": "Helm Highlight Unused Values",
  "description": "Highlights unused values in Helm values.yaml files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [{
      "id": "yaml",
      "extensions": [".yaml", ".yml"]
    }]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "yaml": "^2.3.4"
  }
}